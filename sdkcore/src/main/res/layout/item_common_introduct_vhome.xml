<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <RelativeLayout
        android:id="@+id/rl_introduce_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white_sdk">

        <TextView
            android:id="@+id/tv_introduce_item"
            style="@style/text_bold_14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerInParent="true"
            android:text="@string/setting_key"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_introduce_value"
            style="@style/text_14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dimen_10"
            android:layout_toLeftOf="@+id/tv_introduce_item"
            android:gravity="end"
            tools:text="c6hn" />


    </RelativeLayout>

</layout>