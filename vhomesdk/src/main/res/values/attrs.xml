<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DialogFragmentNavigator">
        <attr name="android:name" />
    </declare-styleable>
    <declare-styleable name="FragmentNavigator">
        <attr name="android:name" />
    </declare-styleable>
    <declare-styleable name="NavHostFragment">
        <attr name="defaultNavHost" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SettingItem">
        <attr name="string_title_setting" format="string" />
        <attr name="string_value" format="string" />
        <attr name="trailingIcon" format="reference" />
        <attr name="is_show_arrow" format="boolean" />
        <attr name="is_show_switch" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AppToolbar">
        <attr name="src_left" format="reference" />
        <attr name="trans_status_bar" format="boolean" />
        <attr name="src_right" format="reference" />
        <attr name="string_right" format="string|reference" />
        <attr name="string_title" format="string|reference" />
        <attr name="left_tint" format="reference" />
        <attr name="src_background" format="reference" />
        <attr name="src_right_color" format="string" />
        <attr name="show_elevation" format="boolean" />
    </declare-styleable>


    <declare-styleable name="CustomEdittext">
        <attr name="title_edittext" format="string|reference" />
        <attr name="src_image_title" format="reference" />
        <attr name="type_edittext" format="reference" />
        <attr name="text_hint_edittext" format="integer" />
        <attr name="is_show_title" format="boolean" />
        <attr name="max_length" format="integer" />
        <attr name="is_drawable_end" format="boolean" />
    </declare-styleable>

    <declare-styleable name="WheelPicker">
        <attr name="fadingEdgeEnabled" format="boolean" />
        <attr name="max" format="integer" />
        <attr name="min" format="integer" />
        <attr name="selectedTextColor" format="color" />
        <attr name="selectedTextScale" format="float" />
        <attr name="timeTextColor" format="color" />
        <attr name="timeTextSize" format="dimension" />
        <attr name="typeface" format="enum">
            <enum name="normal" value="0" />
            <enum name="sans" value="1" />
            <enum name="serif" value="2" />
            <enum name="monospace" value="3" />
        </attr>
        <attr name="wheelItemCount" format="integer" />
        <attr name="wrapSelectorWheel" format="boolean" />
        <attr name="minValidIndex" format="integer" />
        <attr name="maxValidIndex" format="integer" />
        <attr name="align">
            <enum name="LEFT" value="0" />
            <enum name="CENTER" value="1" />
            <enum name="RIGHT" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="IconFeature">
        <attr name="text" format="string"/>
        <attr name="icon" format="reference"/>
        <attr name="state_active" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="TimeLineView">
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="lineColor" format="color" />
        <attr name="lineSpaceWidth" format="dimension" />
        <attr name="lineWidth" format="dimension" />
        <attr name="eventHeight" format="dimension" />
        <attr name="lineMaxHeight" format="dimension" />
        <attr name="lineMidHeight" format="dimension" />
        <attr name="lineMinHeight" format="dimension" />
        <attr name="textMarginTop" format="dimension" />
        <attr name="lineMarginTop" format="dimension" />
        <attr name="alphaEnable" format="boolean" />
        <attr name="minValue" format="float"/>
        <attr name="maxValue" format="float"/>
        <attr name="selectorValue" format="float"/>
        <attr name="perValue" format="float"/>
    </declare-styleable>
    <declare-styleable name="LayoutTimeLineVideo">
        <attr name="show_thumb" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MyTabSwitch">
        <attr name="tab1_title" format="string"/>
        <attr name="tab2_title" format="string"/>
        <attr name="tab1_check" format="boolean"/>
        <attr name="tab_checked_background" format="reference|color"/>
        <attr name="tab_unchecked_background" format="reference|color"/>
        <attr name="text_checked_color" format="reference"/>
        <attr name="text_unchecked_color" format="reference"/>
        <attr name="check_tab1" format="boolean"/>
    </declare-styleable>
</resources>