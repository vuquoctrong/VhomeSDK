<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:id="@android:id/background" >
        <shape
            android:shape="rectangle" >
            <size android:height="@dimen/dimen_16"/>
            <corners
                android:radius="@dimen/dimen_16" />
            <solid android:color="@color/color_progress_seekbar"/>
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape
                android:shape="rectangle" >
                <size android:height="@dimen/dimen_16"/>
                <corners
                    android:radius="@dimen/dimen_16" />
                <solid android:color="@color/color_progress_seekbar"/>
            </shape>
        </clip>
    </item>
</layer-list>